<?xml version="1.0" encoding="utf-8"?>
<com.getstream.sdk.chat.utils.exomedia.p012ui.widget.FitsSystemWindowRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:layout_width="match_parent" android:layout_height="match_parent">
    <LinearLayout android:orientation="vertical" android:id="@+id/exomedia_controls_text_container" android:background="@drawable/stream_exomedia_default_controls_text_background" android:paddingLeft="16dp" android:paddingTop="16dp" android:paddingRight="16dp" android:paddingBottom="16dp" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_alignParentTop="true">
        <TextView android:textSize="16sp" android:textStyle="bold" android:textColor="@android:color/white" android:id="@+id/exomedia_controls_title" android:layout_width="match_parent" android:layout_height="wrap_content"/>
        <TextView android:textSize="14sp" android:textColor="@android:color/white" android:id="@+id/exomedia_controls_sub_title" android:layout_width="match_parent" android:layout_height="wrap_content"/>
        <TextView android:textSize="14sp" android:textColor="@android:color/white" android:id="@+id/exomedia_controls_description" android:layout_width="match_parent" android:layout_height="wrap_content"/>
    </LinearLayout>
    <LinearLayout android:orientation="vertical" android:id="@+id/exomedia_controls_interactive_container" android:background="@drawable/stream_exomedia_default_controls_interactive_background" android:paddingTop="16dp" android:paddingBottom="16dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_alignParentLeft="true" android:layout_alignParentBottom="true" android:layout_alignParentStart="true">
        <LinearLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="16dp" android:layout_marginRight="16dp" android:layout_marginBottom="8dp">
            <TextView android:typeface="monospace" android:textColor="@android:color/white" android:id="@+id/exomedia_controls_current_time" android:paddingLeft="2dp" android:paddingRight="2dp" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
            <SeekBar android:id="@+id/exomedia_controls_video_seek" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="1" style="?android:attr/seekBarStyle"/>
            <TextView android:typeface="monospace" android:textColor="@android:color/white" android:id="@+id/exomedia_controls_end_time" android:paddingLeft="2dp" android:paddingRight="2dp" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
            <LinearLayout android:orientation="horizontal" android:id="@+id/exomedia_controls_extra_container" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
        </LinearLayout>
        <LinearLayout android:gravity="center" android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="16dp" android:layout_marginRight="16dp">
            <ImageButton android:id="@+id/exomedia_controls_previous_btn" android:background="@android:color/transparent" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="@dimen/stream_exomedia_min_button_height" android:minWidth="@dimen/stream_exomedia_min_button_height" android:minHeight="@dimen/stream_exomedia_min_button_height" app:srcCompat="@drawable/stream_exomedia_ic_skip_previous_white"/>
            <ImageButton android:id="@+id/exomedia_controls_play_pause_btn" android:background="@android:color/transparent" android:layout_width="wrap_content" android:layout_height="wrap_content" android:minWidth="@dimen/stream_exomedia_min_button_height" android:minHeight="@dimen/stream_exomedia_min_button_height" app:srcCompat="@drawable/stream_exomedia_ic_play_arrow_white"/>
            <ImageButton android:id="@+id/exomedia_controls_next_btn" android:background="@android:color/transparent" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/stream_exomedia_min_button_height" android:minWidth="@dimen/stream_exomedia_min_button_height" android:minHeight="@dimen/stream_exomedia_min_button_height" app:srcCompat="@drawable/stream_exomedia_ic_skip_next_white"/>
        </LinearLayout>
    </LinearLayout>
    <ProgressBar android:id="@+id/exomedia_controls_video_loading" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_centerInParent="true" style="?android:attr/progressBarStyleLarge"/>
</com.getstream.sdk.chat.utils.exomedia.p012ui.widget.FitsSystemWindowRelativeLayout>
